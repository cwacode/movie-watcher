{
  "name": "workout-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc && npm run build-frontend",
    "dev": "concurrently \"npm run dev-backend\" \"npm run dev-frontend\"",
    "tsc": "npx tsc --watch",
    "start": "npm run build && npm run serve",
    "serve": "concurrently \"nodemon dist/backend/index.js\" \"npm run serve-frontend\"",
    "dev-backend": "npx nodemon dist/backend/index.js",
    "dev-frontend": "cd frontend && npm run dev",
    "build-frontend": "cd frontend && npm run build && rm -rf ../dist/frontend && mv dist ../dist/frontend",
    "serve-frontend": "cd frontend && npm run preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@cypress/webpack-dev-server": "^3.10.0",
    "@types/pg": "^8.10.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "pg": "^8.12.0",
    "vite": "^5.2.13"
  },
  "devDependencies": {
    "@babel/types": "^7.23.6",
    "@cypress/vite-dev-server": "^5.1.1",
    "@cypress/vue": "^3.1.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/vue": "^2.0.0",
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/runtime-dom": "^3.3.11",
    "concurrently": "^8.2.2",
    "cypress": "^13.11.0",
    "nodemon": "^3.1.3",
    "typescript": "^5.4.5",
    "vue": "^3.4.27",
    "vue-router": "^4.3.3"
  },
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
